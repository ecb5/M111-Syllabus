  <exercisegroup cols="3">
    <introduction>
      <p>
        Compute the derivative of each of the following.
      </p>
    </introduction>


    <exercise>
      <statement>
        <p>
          <m>y=\ln(3x)+x</m>
        </p>
      </statement>
      <solution>
        <p>
          <m> \dfdx{y}{x} = \dfrac1x +1</m>  
        </p>
      </solution>
    </exercise>
    <exercise>
      <statement>
        <p>
          <m>y=\frac{1}{\ln(3x)}</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>\dfdx{y}{x} = \dfrac{-1}{x[\ln(3x)]^2}</m>
        </p>
      </solution>
    </exercise>
    <exercise>
      <statement>
        <p>
          <m>y=\frac{1}{\ln(\ln(\ln(x)))}</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>\dfdx{y}{x} = \dfrac{1}{\ln(\ln(x))}\cdot\dfrac{1}{\ln(x)}\cdot\dfrac1x</m>
        </p>
      </solution>
    </exercise>
    <exercise>
      <statement>
        <p>
          <m>y=\sqrt{\ln\left(\sqrt{x}\right)}</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>\dfdx{y}{x} = \dfrac{1}{2\sqrt{2}x\ln(\sqrt{x})}</m>
        </p>
      </solution>
    </exercise>
    <exercise>
      <statement>
        <p>
          <m>y=\ln(\sec(x))</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>\dfdx{y}{x} = \tan(x)</m>
        </p>
      </solution>
    </exercise>
    <exercise>
      <statement>
        <p>
          <m>y=\ln(\sec(x)+\tan(x))</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>\dfdx{y}{x} = \sec(x)</m>
        </p>
      </solution>
    </exercise>
<exercise>
        <p>
          <m>y= x\inverse\sec(x) - \ln(x+\sqrt{x^2-1}</m>
        </p>
<answer>
<p>
          <m>\displaystyle\dfdx{y}{x}=\inverse\sec(x)</m>
        </p>
</answer>
</exercise>

<exercise>
        <p>
          <m>y= x\inverse\tan\left(\frac{x}{2}\right)-\ln\left(x^2+4\right)</m>
        </p>
<answer>
<p>
          <m>\displaystyle\dfdx{y}{x}=\inverse\tan(x)</m>
        </p>
</answer>
</exercise>

  </exercisegroup>


  <exercisegroup cols="2">
    <introduction>
      <p>
        Compute the derivative of each of the following pairs of
        functions.
      </p>
    </introduction>
    <exercise>
      <statement>
        <p>
          <md alignment="alignat">
            <mrow> y=\amp\left[x(x+1)\right]^7</mrow>
            <mrow>z=\amp\ln{\left[x(x+1)\right]^7}</mrow>
          </md>
        </p>
      </statement>
      <solution>
        <p>
          <md alignment="alignat">
            <mrow> \dfdx{y}{x} =\amp
            \left[x(x+1)\right]^7\left[\dfrac7x+\dfrac{7}{x+1}\right]</mrow>
            <mrow>\dfdx{z}{x} =\amp \dfrac7x+\dfrac{7}{x+1}</mrow>
          </md>
        </p>
      </solution>
    </exercise>
    <exercise>
      <statement>
        <p>          <md alignment="alignat">
          <mrow>y=\amp\sqrt{x(x+1)}</mrow>
          <mrow>z=\amp\ln\left(\sqrt{x(x+1)}\right)</mrow>
        </md>
        </p>
      </statement>
      <solution>
        <p>
          <md alignment="alignat">
            <mrow>\dfdx{y}{x} =\amp  \left[\sqrt{x(x+1)}\right]\left[\dfrac{1}{2x}+\dfrac{1}{2(x+1)}\right]</mrow>
            <mrow>\dfdx{z}{x} =\amp \dfrac{1}{2x}+\dfrac{1}{2(x+1)} </mrow>
          </md>
        </p>
      </solution>
    </exercise>
    <exercise>
      <statement>
        <p>          
          <md alignment="alignat">
          <mrow>y=\amp x(x-1)(x-2)</mrow>
          <mrow>z=\amp \ln\left(x(x-1)(x-2)\right)</mrow>
        </md>
        </p>
      </statement>
      <solution>
        <p>
          <md alignment="alignat">
            <mrow>\dfdx{y}{x} =\amp 
            \left[x(x-1)(x-2)\right]\left[\dfrac1x+\dfrac{1}{x-1}+\dfrac{1}{x-2}\right]</mrow>
            <mrow>\dfdx{z}{x} =\amp
            \left(\dfrac1x+\dfrac{1}{x-1}+\dfrac{1}{x-2}\right)</mrow>
          </md>
        </p>
      </solution>
    </exercise>
    <exercise>
      <statement>
        <p>          
          <md alignment="alignat">
          <mrow>y=\amp \frac{1}{x(x-1)(x-2)}</mrow>
          <mrow>z=\amp \ln\left(\frac{1}{x(x-1)(x-2)}\right)</mrow>
        </md>
        </p>
      </statement>
      <solution>
        <p>
          <md alignment="alignat">
            <mrow>\dfdx{y}{x} =\amp 
            -\left[\frac{1}{x(x-1)(x-2)}\right]\left[\dfrac1x+\dfrac{1}{x-1}+\dfrac{1}{x-2}\right]</mrow>
            <mrow>\dfdx{z}{x} =\amp 
            -\left(\dfrac1x+\dfrac{1}{x-1}+\dfrac{1}{x-2}\right)</mrow>
          </md>
        </p>
      </solution>
    </exercise>
    <exercise>
      <statement>
        <p><md alignment="alignat">
          <mrow>y=\amp \ln\left({\frac{x\sqrt{x^2+1}}{(x+1)^{2/3}}}\right)</mrow>
          <mrow>z=\amp \frac{x\sqrt{x^2+1}}{(x+1)^{2/3}}</mrow>
        </md>
        </p>
      </statement>
      <solution>
        <p>
          <md alignment="alignat">
            <mrow>\dfdx{y}{x} =\amp  \dfrac1x+\dfrac{x}{x^2+1}-\dfrac{2}{3(x+1)}</mrow>
            <mrow>\dfdx{z}{x} =\amp \left[\frac{x\sqrt{x^2+1}}{(x+1)^{2/3}}\right]\left[\dfrac1x+\dfrac{x}{x^2+1}-\dfrac{2}{3(x+1)}\right]</mrow>
          </md>
        </p>
      </solution>
    </exercise>
    <exercise>
      <statement>
        <p>
          <md alignment="alignat">
            <mrow>y=\amp \ln\left(\sqrt[3]{\frac{x(x+1)(x-2)}{(x^2+1)(2x+3)}}\right)</mrow>
            <mrow>z=\amp \sqrt[3]{\frac{x(x+1)(x-2)}{(x^2+1)(2x+3)}}</mrow>
          </md>
        </p>
      </statement>
      <solution>
        <p>
          <md alignment="alignat">
            <mrow>\dfdx{y}{x} =\amp
            \dfrac13\left(\dfrac1x+\dfrac{1}{x+1}+\dfrac{1}{x-2} -
            \dfrac{x}{x^2-1}-\dfrac{2}{2x+3}\right)</mrow>
            <mrow>\dfdx{z}{x} =\amp
            \dfrac13\left(\sqrt[3]{\frac{x(x+1)(x-2)}{(x^2+1)(2x+3)}}\right)\left(\dfrac1x+\dfrac{1}{x+1}+\dfrac{1}{x-2}
            -
            \dfrac{x}{x^2-1}-\dfrac{2}{2x+3}\right)</mrow>
          </md>
        </p>
      </solution>
    </exercise>
    <exercise>
      <statement>
        <p>
          <md alignment="alignat">
            <mrow>y=\amp e^{2x}</mrow>
            <mrow>z=\amp 2^x</mrow>
          </md>
        </p>
      </statement>
      <solution>
        <p>
          <md alignment="alignat">
            <mrow>\dfdx{y}{x} =\amp 2e^{2x}</mrow>
            <mrow>\dfdx{z}{x} =\amp \left[\ln(2)\right]\cdot 2^x</mrow>
          </md>
        </p>
      </solution>
    </exercise>
    <exercise>
      <statement>
        <p>
          <md alignment="alignat">
            <mrow>y=\amp \frac{\ln(x)}{2}</mrow>
            <mrow>y=\amp \log_2(x)</mrow>
          </md>
        </p>
      </statement>
      <solution>
        <p>
          <md alignment="alignat">
            <mrow>\dfdx{y}{x} =\amp \dfrac{1}{2x}</mrow>
            <mrow>\dfdx{z}{x} =\amp \dfrac{1}{x\ln(2)}</mrow>
          </md>
        </p>
      </solution>
    </exercise>
  </exercisegroup>

