        <statement>
          <p>
            Area <m>B</m> around the line <m>y=1</m>.
          </p>
        </statement>
        <solution>
          <p>
            <m>\frac{\pi}{12}</m>
          </p>
        </solution>
      </exercise>
      <exercise>
        <statement>
          <p>
            Area <m>C</m> around the line <m>y=1</m>.
          </p>
        </statement>
        <solution>
          <p>
            <m>\frac{\pi}{4}</m>
          </p>
        </solution>
      </exercise>
      <exercise>
        <statement>
          <p>
            Area <m>D</m> around the line <m>y=1</m>.
          </p>
        </statement>
        <solution>
          <p>
            <m>\frac{5\pi}{12}</m>
          </p>
        </solution>
      </exercise>
      <exercise>
        <statement>
          <p>
            Area <m>A</m> around the line <m>x=2</m>.
          </p>
        </statement>
        <solution>
          <p>
            <m>\frac{11\pi}{12}</m>
          </p>
        </solution>
      </exercise>
